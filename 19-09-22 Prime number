#include<stdio.h>
#include<math.h>

int main() {

    int num, i, j, flag=0;    
    printf("\nEnter any number: ");
    scanf("%d",&num);
    
    for(i=2;i<sqrt(num);i++){
        if(num%i==0){
            flag=1;
            break;
        }
    }
    
    if(flag==1){
        printf("\n Not a Prime number");
    }
    else{
        printf("\n prime number");
    }
    return 0;
}

#include <stdio.h>

int main() {
    
    int num1,num2, sum, sub, multi, divide;
     
    printf("\nEnter num1:");
    scanf("%d",&num1);
    
    printf("\nEnter num1:");
    scanf("%d",&num2);
    
    printf("\n----------------------------------\n");
    sum = num1 + num2;
    printf("The sum of %d and %d is: %d", num1, num2, sum);
    
    printf("\n----------------------------------\n");
    sub  = num1 - num2;
    printf("The sub of %d and %d is: %d", num1, num2, sub);
    
    printf("\n----------------------------------\n");
    multi = num1 * num2;
    printf("The multi of %d and %d is: %d", num1, num2, multi);
    
    printf("\n----------------------------------\n");
    divide = num1 / num2;
    printf("The divide  of %d and %d is: %d", num1, num2, divide);
    

    return 0;
}
